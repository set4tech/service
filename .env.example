# Anthropic API
ANTHROPIC_API_KEY=your_anthropic_api_key

# OpenAI API
OPENAI_API_KEY=your_openai_api_key

# Google/Gemini API
GEMINI_API_KEY=your_gemini_api_key
GOOGLE_API_KEY=your_google_api_key
NEXT_PUBLIC_GOOGLE_MAPS_API_KEY=your_google_maps_api_key

# AWS S3
AWS_PROFILE=dev
AWS_ACCOUNT_ID=your_aws_account_id
AWS_DEFAULT_REGION=us-east-1
AWS_ACCESS_KEY_ID=your_aws_access_key_id
AWS_SECRET_ACCESS_KEY=your_aws_secret_access_key
AWS_REGION=us-east-1
AWS_S3_BUCKET_NAME=your_s3_bucket_name
NEXT_PUBLIC_S3_BUCKET_NAME=your_s3_bucket_name

# Supabase
NEXT_PUBLIC_SUPABASE_URL=https://your_project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key
SUPABASE_PASSWORD=your_supabase_password
SUPABASE_USER=postgres
SUPABASE_DB=postgres
SUPABASE_URL=db.your_project.supabase.co
SUPABASE_PORT=5432
SUPABASE_DB_HOST=aws-0-us-east-1.pooler.supabase.com
SUPABASE_DB_USER=postgres.your_project
SUPABASE_DB_PASSWORD=your_supabase_password
SUPABASE_DB_NAME=postgres

# Redis (optional - for caching)
REDIS_URL=redis://default:password@your-redis-host:port

# Application URLs
NEXT_PUBLIC_APP_URL=http://localhost:3000
NEXT_PUBLIC_API_URL=http://localhost:3000
NEXT_PUBLIC_API_ENDPOINT=https://your-api-endpoint.io

# Session Secret (for iron-session - REQUIRED FOR PASSWORD PROTECTION)
# Generate a random 32+ character string
SESSION_SECRET=your_complex_password_at_least_32_characters_long_for_security

# Team Member Authentication (for middleware)
# Set this to true in Vercel environment variables for team members
VERCEL_TEAM_MEMBER=false

# Admin Access Key (optional - alternative to team member env var)
# Set this to a secret key that team members can use in x-admin-key header
ADMIN_ACCESS_KEY=your_admin_access_key

# Helicone (API monitoring - optional)
HELICONE_API_KEY=your_helicone_api_key

# Vercel (optional)
VERCEL_TOKEN=your_vercel_token

# Roboflow (optional - for floor plan detection)
ROBOFLOW_API_KEY=your_roboflow_api_key
ROBOFLOW_MODEL_ID=your_roboflow_model_id
